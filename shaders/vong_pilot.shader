models/players/vong_pilot/uv
{
       cull twosided
    {
        map models/players/vong_pilot/uv
        rgbGen lightingDiffuse
        //alphaFunc GE128
    }
    {
        map models/players/vong_pilot/uv
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
	detail
    }
}

