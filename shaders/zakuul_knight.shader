models/players/Zakuul_Knight/head
{
	cull	twosided
    {
        map models/players/Zakuul_Knight/head
        rgbGen lightingDiffuse
        //alphaFunc GE128
    }
    {
        map models/players/Zakuul_Knight/head_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
	detail
    }
    {
        map models/players/Zakuul_Knight/glow
        blendFunc GL_ONE GL_ONE
        detail
        rgbGen identity
    }
}

